{
  "expo": {
    "name": "Ecohomely",
    "slug": "Ecohomely",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecohomely.user",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBPLaShGKneZBd5NQBXoDsjoryw7ekDKfc"
        }
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ecohomely.user",
      "googleServicesFile": "./google-services.json",
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "enableProguardInReleaseBuilds": false,
            "enableShrinkResourcesInReleaseBuilds": false,
            "enableAndroidX": true,
            "enableJetifier": true,
            "gradleProperties": {
              "android.useAndroidX": "true",
              "android.enableJetifier": "true",
              "android.enableR8": "false",
              "android.enableDexingArtifactTransform": "false"
            }
          }
        }
      ],
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow the app to access the microphone for voice search.",
          "speechRecognitionPermission": "Allow the app to recognize speech for voice search."
        }
      ],
      "expo-localization",
      "expo-font"
    ],
    "runtimeVersion": "1.1.0",
    "extra": {
      "eas": {
        "projectId": "91eec2d0-c9d4-4a3c-aad9-c21ffd501e3e"
      }
    },
    "owner": "ecohomely-apps"
  }
}
