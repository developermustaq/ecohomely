{
  "expo": {
    "name": "Ecohomely",
    "slug": "Ecohomely",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecohomely.user",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBPLaShGKneZBd5NQBXoDsjoryw7ekDKfc"
        }
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ecohomely.user",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow the app to access the microphone for voice search.",
          "speechRecognitionPermission": "Allow the app to recognize speech for voice search."
        }
      ],
      "expo-localization"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "ae2439ae-a14f-4d0b-96de-3b624f5b2a10"
      }
    },
    "owner": "mustaq21"
  }
}
